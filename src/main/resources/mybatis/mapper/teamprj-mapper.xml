<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="com.prj.teamprj.dao.ITeamPrjDao">
	<select id="getList" resultType="com.prj.teamprj.dto.MyPageDto">
		SELECT * FROM team_board
	</select>
	
	<insert id="write">
		INSERT INTO team_board (title, content) VALUES
		(#{dto.title}, #{dto.content})
	</insert>
	
	<select id="getDto">
		SELECT * FROM team_board WHERE bno = #{bno}
	</select>
	
	<delete id="delete">
		DELETE FROM team_board WHERE bno = #{bno}
	</delete>
  </mapper>